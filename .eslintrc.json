{
    "env": {
        "node":true,
        "jest/globals": true,
        "es6": true
    },
    "globals": {
        "kintone": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "sourceType": "module"
    },
    "plugins": [
        "jest",
        "@typescript-eslint"
    ],
    "extends": [
        "eslint:recommended"
    ],
    "rules": {
        "no-unused-vars": "off",
        "@typescript-eslint/no-unused-vars": "error"
    }
}
